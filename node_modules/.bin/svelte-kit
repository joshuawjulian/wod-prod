#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/app/node_modules/.pnpm/@sveltejs+kit@2.27.3_@sveltejs+vite-plugin-svelte@6.1.1_svelte@5.38.0_vite@7.1.1_@types_89406837e5336440dff111589740c30b/node_modules/@sveltejs/kit/node_modules:/app/node_modules/.pnpm/@sveltejs+kit@2.27.3_@sveltejs+vite-plugin-svelte@6.1.1_svelte@5.38.0_vite@7.1.1_@types_89406837e5336440dff111589740c30b/node_modules/@sveltejs/node_modules:/app/node_modules/.pnpm/@sveltejs+kit@2.27.3_@sveltejs+vite-plugin-svelte@6.1.1_svelte@5.38.0_vite@7.1.1_@types_89406837e5336440dff111589740c30b/node_modules:/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/app/node_modules/.pnpm/@sveltejs+kit@2.27.3_@sveltejs+vite-plugin-svelte@6.1.1_svelte@5.38.0_vite@7.1.1_@types_89406837e5336440dff111589740c30b/node_modules/@sveltejs/kit/node_modules:/app/node_modules/.pnpm/@sveltejs+kit@2.27.3_@sveltejs+vite-plugin-svelte@6.1.1_svelte@5.38.0_vite@7.1.1_@types_89406837e5336440dff111589740c30b/node_modules/@sveltejs/node_modules:/app/node_modules/.pnpm/@sveltejs+kit@2.27.3_@sveltejs+vite-plugin-svelte@6.1.1_svelte@5.38.0_vite@7.1.1_@types_89406837e5336440dff111589740c30b/node_modules:/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@sveltejs+kit@2.27.3_@sveltejs+vite-plugin-svelte@6.1.1_svelte@5.38.0_vite@7.1.1_@types_89406837e5336440dff111589740c30b/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../.pnpm/@sveltejs+kit@2.27.3_@sveltejs+vite-plugin-svelte@6.1.1_svelte@5.38.0_vite@7.1.1_@types_89406837e5336440dff111589740c30b/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
fi
